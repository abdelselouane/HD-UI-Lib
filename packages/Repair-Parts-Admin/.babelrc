{
  "presets": ["env", "react"],
  "plugins": [
    [
      "transform-class-properties",
      ["transform-runtime", {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }],
      "transform-es2015-arrow-functions",
      "transform-es2015-spread",
      { "spec": true,
        "loose": true }
    ]
  ]
}
